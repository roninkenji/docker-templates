<?xml version="1.0" encoding="utf-8"?>
<Container>
	<Name>SlackBase</Name>
	<Description>
[b]Slackware management userspace for unRAID[/b]
	</Description>
	<Registry>https://registry.hub.docker.com/u/roninkenji/slackware-base</Registry>
	<GitHub>https://github.com/ronin-kenji/slackware-base</GitHub>
	<Repository>roninkenji/slackware-base</Repository>
	<BindTime>true</BindTime>
	<Privileged>false</Privileged>
	<Environment/>
	<Networking>
		<Mode>bridge</Mode>
		<Publish>
			<Port>
			</Port>
		</Publish>
	</Networking>
	<Data>
		<Volume>
			<HostDir>/etc/localtime</HostDir>
			<ContainerDir>/etc/localtime</ContainerDir>
			<Mode>ro</Mode>
		</Volume>
		<Volume>
			<HostDir>/mnt</HostDir>
			<ContainerDir>/mnt</ContainerDir>
			<Mode>rw</Mode>
		</Volume>
	</Data>
	<WebUI/>
	<Banner/>
	<Icon/>
</Container>

